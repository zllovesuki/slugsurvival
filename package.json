{
  "name": "slugsurvival",
  "version": "3.25.7",
  "scripts": {
    "dev": "node node_modules/watchify/bin/cmd.js -vd -p browserify-hmr -e src/main.js -o public/app.js -v",
    "test": "node node_modules/.bin/mocha --timeout 30000 --reporter spec",
    "prod": "gulp && NODE_ENV=production node ./node_modules/browserify/bin/cmd.js -g envify -e src/main.js | node ./node_modules/uglify-js/bin/uglifyjs -c warnings=false -m > public/prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.fm:zllovesuki/slugsurvival.git"
  },
  "author": "Rachel Chen <me@rachelchen.me>",
  "license": "GPL-3.0",
  "dependencies": {
    "body-parser": "^1.18.2",
    "debug": "~3.1.0",
    "express": "^4.16.3",
    "socket.io-client": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "browserify": "^16.1.1",
    "browserify-hmr": "^0.3.6",
    "compound-subject": "0.0.2",
    "envify": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.3",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "localforage": "^1.7.1",
    "lodash.debounce": "^4.0.8",
    "lunr": "^2.1.6",
    "mobile-detect": "^1.4.1",
    "stringify": "^5.2.0",
    "superagent": "^3.8.2",
    "uglify-js": "^3.3.18",
    "v-tooltip": "^2.0.0-rc.31",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vue-resource": "^1.5.0",
    "vue-router": "^3.0.1",
    "vueify": "^9.4.1",
    "vueify-insert-css": "^1.0.0",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "watchify": "^3.11.0"
