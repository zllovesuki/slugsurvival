    "debug": "~2.2.0",
    "express": "~4.14.0"
  },
  "devDependencies": {
    "alertify.js": "^1.0.12",
    "babel-core": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "browserify": "^13.1.0",
    "elasticlunr": "^0.9.5",
    "localforage": "^1.4.3",
    "lodash.debounce": "^4.0.8",
    "nanobar": "^0.4.2",
    "stringify": "^5.1.0",
    "uglifyjs": "^2.4.10",
    "vue": "^2.0.1",
    "vue-hot-reload-api": "^2.0.6",
    "vue-resource": "^1.0.3",
