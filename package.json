{
  "name": "slugsurvival",
  "version": "3.9.0",
  "scripts": {
    "dev": "node node_modules/watchify/bin/cmd.js -vd -p browserify-hmr -e src/main.js -o public/app.js -v",
    "test": "node node_modules/.bin/mocha --timeout 30000 --reporter spec",
    "prod": "node node_modules/cross-env/bin/cross-env.js NODE_ENV=production node node_modules/browserify/bin/cmd.js -g envify -e src/main.js | node ./node_modules/uglifyjs/bin/uglifyjs -c warnings=false -m > public/prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.fm:zllovesuki/slugsurvival.git"
  },
  "author": "Rachel Chen <me@rachelchen.me>",
  "license": "GPL-3.0",
  "dependencies": {
    "body-parser": "^1.15.2",
    "debug": "~2.6.0",
    "express": "~4.14.0",
    "fill-pdf": "^0.5.0"
  },
  "devDependencies": {
    "alertify.js": "^1.0.12",
    "babel-core": "^6.18.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "bluebird": "^3.4.6",
    "browserify": "^13.1.1",
    "browserify-hmr": "^0.3.5",
    "compound-subject": "0.0.1",
    "cross-env": "^3.1.3",
    "envify": "^4.0.0",
    "localforage": "^1.4.3",
    "lodash.debounce": "^4.0.8",
    "lunr": "^0.7.2",
    "mobile-detect": "^1.3.5",
    "nanobar": "^0.4.2",
    "stringify": "^5.1.0",
    "uglifyjs": "^2.4.10",
    "vue": "^2.0.7",
    "vue-hot-reload-api": "^2.0.6",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.3",
    "vueify": "^9.3.0",
    "vueify-insert-css": "^1.0.0",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^4.1.0",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "stringify"
    ]
  }
}
