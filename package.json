{
  "name": "slugsurvival",
  "version": "2.0.5",
  "scripts": {
    "test": "node node_modules/.bin/mocha --timeout 30000 --reporter spec",
    "prod": "NODE_ENV=production node node_modules/browserify/bin/cmd.js -t stringify -t vueify -e src/main.js | ./node_modules/uglifyjs/bin/uglifyjs -c -m > public/prod.js",
    "watch": "node node_modules/watchify/bin/cmd.js -t stringify -t vueify -e src/main.js -o public/app.js -v"
  },
